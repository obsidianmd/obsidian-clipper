{
	"name": "obsidian-clipper",
	"version": "0.8.0",
	"description": "Official web clipper for Obsidian.",
	"scripts": {
		"dev:chrome": "webpack --watch --env BROWSER=chrome",
		"dev:firefox": "webpack --watch --env BROWSER=firefox",
		"build:chrome": "webpack --env BROWSER=chrome",
		"build:firefox": "webpack --env BROWSER=firefox",
		"build:safari": "webpack --env BROWSER=safari",
		"build": "npm run build:chrome && npm run build:firefox && npm run build:safari",
		"dev": "npm run dev:chrome"
	},
	"devDependencies": {
		"@types/chrome": "^0.0.270",
		"@types/lz-string": "^1.5.0",
		"@types/node": "^22.5.4",
		"@types/turndown": "^5.0.5",
		"@types/webextension-polyfill": "^0.12.1",
		"copy-webpack-plugin": "^12.0.2",
		"css-loader": "^7.1.2",
		"mini-css-extract-plugin": "^2.9.1",
		"sass": "^1.77.8",
		"sass-loader": "^16.0.1",
		"ts-loader": "^9.5.1",
		"typescript": "^5.5.4",
		"webextension-polyfill": "^0.12.0",
		"webpack": "^5.94.0",
		"webpack-cli": "^5.1.4",
		"zip-webpack-plugin": "^4.0.1"
	},
	"dependencies": {
		"@mozilla/readability": "^0.5.0",
		"@types/mozilla-readability": "^0.2.1",
		"@types/safari-extension": "^0.0.33",
		"dayjs": "^1.11.13",
		"lucide": "^0.359.0",
		"lz-string": "^1.5.0",
		"mathml-to-latex": "^1.4.1",
		"turndown": "^7.2.0",
		"turndown-plugin-gfm": "^1.0.2"
	}
}
